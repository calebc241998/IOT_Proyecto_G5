<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9D9D9"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="164dp"
        android:layout_height="54dp"
        tools:layout_editor_absoluteX="256dp"
        tools:layout_editor_absoluteY="275dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/buttonReportar"
            android:layout_width="110dp"
            android:layout_height="38dp"
            android:backgroundTint="#FFFFFF"
            android:text="Reportar"
            android:textAlignment="textStart"
            android:textColor="#FF9800"
            app:cornerRadius="5dp"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="8dp"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="29dp"
            android:layout_height="26dp"
            app:srcCompat="@drawable/precaucion"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="14dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="164dp"
        android:layout_height="54dp"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="275dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/buttonBorrar"
            android:layout_width="96dp"
            android:layout_height="38dp"
            android:backgroundTint="#FFFFFF"
            android:text="Borrar"
            android:textAlignment="textStart"
            android:textColor="#D33535"
            app:cornerRadius="5dp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="8dp"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="27dp"
            android:layout_height="22dp"
            app:srcCompat="@drawable/eliminar"
            tools:layout_editor_absoluteX="96dp"
            tools:layout_editor_absoluteY="16dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="164dp"
        android:layout_height="54dp"
        tools:layout_editor_absoluteX="-8dp"
        tools:layout_editor_absoluteY="275dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/buttonEditar"
            android:layout_width="96dp"
            android:layout_height="38dp"
            android:backgroundTint="#FFFFFF"
            android:text="Editar"
            android:textAlignment="textStart"
            android:textColor="#54CCC5"
            app:cornerRadius="5dp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="8dp"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/baseline_edit_24"
            tools:layout_editor_absoluteX="96dp"
            tools:layout_editor_absoluteY="13dp"
            app:tint="#54CCC5" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <FrameLayout
        android:layout_width="402dp"
        android:layout_height="231dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.777"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="362dp"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="SKU"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="CFI-1000A01"
                        android:textColor="#000000"
                        android:textSize="20dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Número de serie"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="PLA-NIK-AZU-S"
                        android:textColor="#000000"
                        android:textSize="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Marca"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="CISCO"
                        android:textColor="#000000"
                        android:textSize="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Modelo"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="IR8100"
                        android:textColor="#000000"
                        android:textSize="20dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Descripción"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="Nuevo"
                        android:textColor="#000000"
                        android:textSize="20dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Fecha de registro"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="01/04/2024"
                        android:textColor="#000000"
                        android:textSize="20dp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Fecha de edición"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:text="01/04/2024"
                        android:textColor="#000000"
                        android:textSize="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Fotos del equipo"
                        android:textColor="#000000"

                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <ImageView
                        android:id="@+id/imageView10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/multimedia" />

                </LinearLayout>

            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </FrameLayout>

    <TextView
        android:id="@+id/textViewTitulo"
        android:layout_width="240dp"
        android:layout_height="59dp"
        android:layout_gravity="center_vertical"
        android:gravity="center"
        android:text="@string/rea_telecom"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="125dp"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <include
        android:id="@+id/include"
        layout="@layout/navbar"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="0dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="246dp"
        android:layout_height="66dp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="184dp"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="185dp"
            android:layout_height="39dp"
            android:backgroundTint="#FFFFFF"
            app:cardCornerRadius="20dp"
            tools:layout_editor_absoluteX="34dp"
            tools:layout_editor_absoluteY="16dp"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/textViewDescripcion"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:text="Descripción de equipo"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds" />

        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9D9D9"
    tools:context=".MainActivity">


    <EditText
        android:id="@+id/SALsearchBar"
        android:layout_width="353dp"
        android:layout_height="61dp"
        android:background="#FFFFFF"
        android:drawableStart="@android:drawable/ic_menu_search"
        android:drawablePadding="10dp"
        android:hint="Busque por usuario"
        android:padding="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.319" />

    <FrameLayout
        android:layout_width="402dp"
        android:layout_height="350dp"
        android:paddingStart="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="297dp">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/SALtitulotabla1"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/fondo"
                        android:gravity="center"
                        android:outlineProvider="none"
                        android:text="Notificaciones"
                        android:textColor="#000000"

                        android:textSize="20sp"
                        android:textStyle="bold" />

                </LinearLayout>

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/SALlinea1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/SALtextlinea1"
                                    android:layout_width="382dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:background="@drawable/fondo"
                                    android:gravity="center|left"
                                    android:outlineProvider="none"

                                    android:text="El usuario administrador Ana Lara ha suspendido al usuario supervisor William Espinoza."
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/SALlinea2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/SALtextlinea2"
                                    android:layout_width="382dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:background="@drawable/fondo"
                                    android:gravity="center|left"
                                    android:outlineProvider="none"

                                    android:text="El usuario supervisor bien espinosa ha agregado un equipo al sitio Plaza San Miguel."
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/SALlinea3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/SALtextlinea3"
                                    android:layout_width="382dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:background="@drawable/fondo"
                                    android:gravity="center|left"
                                    android:outlineProvider="none"

                                    android:text="El usuario supervisor William Espinoza ha creado un nuevo equipo."
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/SALlinea4"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/SALtextlinea4"
                                    android:layout_width="382dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:background="@drawable/fondo"
                                    android:gravity="center|left"
                                    android:outlineProvider="none"

                                    android:text="El usuario supervisor William Espinoza ha creado el sitio Plaza San Miguel."
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/SALlinea5"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/SALtextlinea5"
                                    android:layout_width="382dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:background="@drawable/fondo"
                                    android:gravity="center|left"
                                    android:outlineProvider="none"

                                    android:text="El usuario administrador Ana Lara ha creado al usuario supervisor William Espinoza."
                                    android:textColor="#000000"
                                    android:textSize="17sp"
                                    />

                            </LinearLayout>

                        </LinearLayout>



                    </LinearLayout>
                </ScrollView>

            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </FrameLayout>

    <TextView
        android:id="@+id/SALtextView1"
        android:layout_width="240dp"
        android:layout_height="59dp"
        android:layout_gravity="center_vertical"
        android:gravity="center"
        android:text="Área TELECOM"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="89dp" />

    <include
        android:id="@+id/SALinclude"
        layout="@layout/superadmin_navbar"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="0dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="270dp"
        android:layout_height="35dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="148dp">

        <androidx.cardview.widget.CardView
            android:id="@+id/SALcardView2"
            android:layout_width="272dp"
            android:layout_height="37dp"
            android:backgroundTint="#FFFFFF"
            app:cardCornerRadius="20dp">

            <TextView
                android:id="@+id/SALtextView2"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:gravity="center"
                android:text="Logs"
                android:textColor="@android:color/black"
                android:textSize="25sp"
                android:textStyle="bold" />
        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>